<p id="notice"><%= notice %></p>

<%= link_to 'ログアウト', destroy_admin_user_session_path, method: :delete %>

<h1>Admin User Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_user_posts.each do |admin_user_post| %>
      <tr>
        <td><%= admin_user_post.title %></td>
        <td><%= admin_user_post.body %></td>
        <td><%= link_to 'Show', admin_user_post %></td>
        <td><%= link_to 'Edit', edit_admin_user_post_path(admin_user_post) %></td>
        <td><%= link_to 'Destroy', admin_user_post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin User Post', new_admin_user_post_path %>
